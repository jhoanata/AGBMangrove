	Doi = {10.1002/joc.1276},
	File = {:Users/jbolivar/Downloads/Hijmans et al. (2005).pdf:pdf},
	Isbn = {1097-0088},
	Issn = {08998418},
	Journal = {International Journal of Climatology},
	Keywords = {ANUSPLIN,Climate,Climatology,Error,GIS,Interpolation,Land areas,Precipitation,Temperature,Uncertainty,WorldClim database},
	Mendeley-Tags = {Land areas,Climatology,WorldClim database},
	Number = {15},
