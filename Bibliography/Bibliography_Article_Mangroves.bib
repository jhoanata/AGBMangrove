	Issn = {0304-3770},
	Journal = {Aquatic Botany},
	Keywords = {Net primary production},
	Note = {Mangrove Ecology -- Applications in Forestry and Costal Zone Management},
	Number = {2},
	Pages = {128 - 137},
	Title = {Allometry, biomass, and productivity of mangrove forests: A review},
	Url = {http://www.sciencedirect.com/science/article/pii/S0304377007001829},
	Volume = {89},
	Year = {2008},
