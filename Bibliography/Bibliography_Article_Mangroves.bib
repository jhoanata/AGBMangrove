	Doi = {http://dx.doi.org/10.1016/j.foreco.2013.08.045},
	Issn = {0378-1127},
	Journal = {Forest Ecology and Management},
	Keywords = {South China},
	Pages = {539 - 546},
	Title = {Ecosystem carbon stocks of mangrove forest in Yingluo Bay, Guangdong Province of South China},
