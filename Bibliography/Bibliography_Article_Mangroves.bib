	Journal = {Forest Ecology and Management},
	Keywords = {Kenya},
	Pages = {968 - 982},
	Title = {Propagating uncertainty to estimates of above-ground biomass for Kenyan mangroves: A scaling procedure from tree to landscape level},
	Url = {http://www.sciencedirect.com/science/article/pii/S0378112713006610},
