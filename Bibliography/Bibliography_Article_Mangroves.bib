	Isbn = {1748-9326},
	Issn = {17489326},
	Journal = {Environmental Research Letters},
	Keywords = {REDD,Uncertainty,accounting,accuracy,conservative estimates,deforestation,uncertainty},
	Mendeley-Tags = {Uncertainty,REDD},
