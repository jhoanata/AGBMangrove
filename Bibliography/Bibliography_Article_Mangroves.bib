	Isbn = {0378-1127},
	Issn = {03781127},
	Journal = {Forest Ecology and Management},
	Keywords = {Biomass,Colombia,EVI,MODIS,Model,Remote sensing,Tropics,VCF},
	Mendeley-Tags = {Biomass,Colombia,EVI,Model,Remote sensing},
	Number = {4},
	Pages = {1237--1246},
	Title = {{Aboveground biomass assessment in Colombia: A remote sensing approach}},
	Volume = {257},
